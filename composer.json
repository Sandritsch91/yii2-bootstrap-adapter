{
    "name": "sandritsch91/yii2-bootstrap-adapter",
    "type": "yii2-extension",
    "description": "Bootstrap adapter for yii2",
    "keywords": [
        "yii2",
        "bootstrap",
        "bootstrap3",
        "bootstrap4",
        "bootstrap5"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sandritsch91",
            "email": "sandritsch91@gmail.com"
        }
    ],
    "support": {
		"source": "https://github.com/sandritsch91/yii2-bootstrap-adapter",
		"issues": "https://github.com/sandritsch91/yii2-bootstrap-adapter/issues"
    },
    "minimum-stability": "beta",
    "require": {
        "php": ">=5.6",
        "yiisoft/yii2": "^2.0.20"
    },
    "require-dev": {
        "yiisoft/yii2-bootstrap": "2.0.11",
        "yiisoft/yii2-bootstrap4": "*",
        "yiisoft/yii2-bootstrap5": "*"
    },
    "autoload": {
        "psr-4": {
            "sandritsch91\\yii2\\bootstrapAdapter\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
